{
  "version": "1.0.0-*",
  "authors": [ "Malachi Burke" ],
  "description": "Android extensions for Fact.Extensions.Collection, including PreferencesBag",
  "packOptions": {
    "releaseNotes": ""
  },

  "buildOptions": {
    "xmlDoc": "true",
    /* Disable warnings about malformed XmlDoc stuff */
    "nowarn": [ "1591", "1571", "1572", "1573", "1574", "1587" ]
  },

  "dependencies": {
    "Fact.Extensions.Collection": "1.0.0-alpha011",
    "Fact.Extensions.Serialization": "1.0.0-alpha011"
  },

  "frameworks": {
    "monoandroid5.0": {
      "frameworkAssemblies": {
        "Mono.Android": "*",
        "System": "2.*",
        "Java.Interop": "0.1.0.0",
        "mscorlib": "2.*"
      }
    }
  }
}
